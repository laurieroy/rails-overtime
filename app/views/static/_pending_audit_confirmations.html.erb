<% pending_audit_confirmations.each do |pac| %>
<%= link_to "I confirm that I did not perform any overtime for the week of: #{ pac.start_date }",
		confirm_audit_log_path(pac), 
		id: "confirm_#{pac.id}", 
		class: "btn btn-primary btn-lg btn-block",
		data: { confirm: "I CONFIRM I did not work any OT the week #{ pac.start_date }-#{ pac.start_date + 6 }."} 
	%>
<% end %>